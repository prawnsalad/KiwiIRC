    // Normalise '.' and '..' from paths
    function normalisePath(path) {
        var parts = path.splice ? path : path.split('/');
        var normalised = [];
        for (var i=0; i<parts.length; i++) {
            if (parts[i] === '..') {
                normalised.pop();
            } else if (parts[i] === '.') {
